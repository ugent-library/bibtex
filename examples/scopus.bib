@article{VanPuyvelde2023,
	title        = {Park features that encourage park visitation among older adults: A qualitative study},
	author       = {Van Puyvelde, Amber and Deforche, Benedicte and Mertens, Lieze and Rivera, Elise and Van Dyck, Delfien and Veitch, Jenny and Poppe, Louise},
	year         = 2023,
	journal      = {Urban Forestry and Urban Greening},
	publisher    = {Elsevier GmbH},
	volume       = 86,
	doi          = {10.1016/j.ufug.2023.128026},
	issn         = 16188667,
	url          = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85165104991\&doi=10.1016\%2fj.ufug.2023.128026\&partnerID=40\&md5=a3bf23b45df931036b557248994ea7a9},
	note         = {Cited by: 0},
	affiliations = {Research Group Health Promotion, Department of Public Health and Primary Care, Faculty of Medicine and Health Sciences, Ghent Universit, Corneel Heymanslaan 10, Ghent, 9000, Belgium; Reserach Group Physical Activity and Health, Department of Movement and Sports Sciences, Faculty of Medicine and Health Sciences, Ghent University, Watersportlaan 2, Ghent, 9000, Belgium; Deakin University, Geelong, Australia, Institute for Physical Activity and Nutrition (IPAN), School of Exercise and Nutrition Sciences, 221 Burwood Highway, Burwood, 3125, VIC, Australia; Department of Movement and Sport Sciences, Vrije Universiteit Brussel, Pleinlaan 2, Brussels, 1050, Belgium},
	abstract     = {Background: Parks provide many opportunities for promoting physical, social, and mental wellbeing among older adults and are therefore considered a good setting to promote healthy ageing. However, older adults are the age group least frequently observed visiting parks. To attract more older adults to parks, it is important to understand their needs and preferences for park design. The aim of this qualitative study was to gain in-depth information about older adults' preferred park features for park visitation. Methods: Semi-structured interviews were conducted with 25 older adults aged > 60 years who visited a recently renewed urban park in Ghent, Belgium at least once since its renewal. The interviews were conducted in-park or in the nursing home adjacent to the park until saturation of information was obtained. All interviews were transcribed verbatim, pseudonymised, and analysed by inductive content analysis using NVivo 12 software. Results: The most preferred park activities were walking, sitting, and watching the environment. The park features liked by older adults were good quality and accessible walking paths, nature and vegetation, openness and structural layout of the park, and sufficient and good quality sitting areas. The park features that they did not like mostly related to poor park maintenance. Conclusions: This study provides an overview of park features that older adults perceive as important for park visitation. It is the first qualitative study in Belgium to assess which park features entice older adults to visit parks, and it contributes to the limited European knowledge on this topic. The evidence can help urban planners, landscape architects, and policymakers to (re)design attractive local parks that promote healthy ageing. \textcopyright{} 2023 Elsevier GmbH},
	author_keywords = {Healthy ageing; Park characteristics; Park renewal; Physical activity; Social interaction},
	keywords     = {Belgium; East Flanders; Ghent; aging; elderly population; park design; physical activity; public health; social behavior},
	correspondence_address = {A. Van Puyvelde; Department of Public Health and Primary Care (GE39) – Ghent University, Research Group Health Promotion, UZ Gent, Ghent, 4K3 – Ingang 42, Corneel Heymanslaan 10, 9000, Belgium; email: amber.vanpuyvelde\@ugent.be},
	language     = {English},
	abbrev_source_title = {Urban For. Urban Greening},
	type         = {Article},

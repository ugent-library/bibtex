	correspondence_address = {J. Heerman; Department of Anaesthesia and Intensive Care, AZ Maria Middelares Hospital, Ghent, Buitenring Sint-Denijs 30, 9000, Belgium; email: jan.heerman\@azmmsj.be},
	language     = {English},
	abbrev_source_title = {PharmacoEcon. Open},
	type         = {Article},
	publication_stage = {Final},
	source       = {Scopus}
}
